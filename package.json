  "main": "server.js",
  "scripts": {
    "server": "nodemon server.js --exec babel-node --presets babel-preset-env",
    "start": "node server.js",
    "client": "cd client && npm start",
    "build-client": "cd client && npm run build",
    "install-client": "cd client && npm i",
    "heroku-postbuild": "npm run install-client && npm run build-client",
    "dev": "concurrently -n 'server,client' -c 'green,blue' \"npm run server\" \"npm run client\"",
    "lint": "eslint src --ext .js,.jsx client/src --ext .js,.jsx server.js",
    "lint-fix": "eslint src --ext .js,.jsx client/src --ext .js,.jsx server.js --fix"
  },
  "repository": {
    "type": "git",
